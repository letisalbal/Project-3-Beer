Table: public.brewerylocations

DROP TABLE public.brewerylocations;

CREATE TABLE public.brewerylocations
(
    id integer NOT NULL DEFAULT nextval('brewerylocations_id_seq'::regclass),
    name character varying COLLATE pg_catalog."default",
    brewery_type character varying COLLATE pg_catalog."default",
    street character varying COLLATE pg_catalog."default",
    city character varying COLLATE pg_catalog."default",
    state character varying COLLATE pg_catalog."default",
    postal_code character varying COLLATE pg_catalog."default",
    country character varying COLLATE pg_catalog."default",
    longitude numeric,
    latitude numeric,
    phone bigint,
    website_url character varying COLLATE pg_catalog."default",
    updated_at date,
    CONSTRAINT brewerylocations_pkey PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;


